<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alwaysbfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Gain_muscles_tabbed"
            android:label="@string/title_activity_gain_muscles_tabbed"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".ChooseProgramActivity" />
        <activity
            android:name=".Lose_weights_tabbed"
            android:exported="true"
            android:label="@string/title_activity_lose_weights_tabbed"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NewPasswordActivity"
            android:exported="true"
            android:label="newpassword"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SignUpActivity"
            android:exported="true"
            android:label="Register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>